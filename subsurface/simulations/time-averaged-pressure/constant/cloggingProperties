FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
// [kg m s K mol * *]

// Clogging parameters
clog?     true;

n_c       n_c       [0 0 0 0 0 0 0]    0.05;      // Minimum porosity
perm_c    perm_c    [0 2 0 0 0 0 0]    1.135E-13;  // Minimum permeability

n_0       n_0       [0 0 0 0 0 0 0]    0.358;     // Reference porosity
perm_0    perm_0    [0 2 0 0 0 0 0]    9.119E-11;  // Reference permeability




