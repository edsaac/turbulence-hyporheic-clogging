FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "system";
    object          controlDict;
}

application     biocloggingFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1123200;

writeInterval   3600;

deltaT          120;

writeControl    runTime;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      fixed;

timePrecision   2;

runTimeModifiable false;

adjustTimeStep  no;

maxCo           1;

debug           1;

functions
{
    fieldAverageOverTime
    {
        type            fieldAverage;
        libs            ( "libfieldFunctionObjects.so" );
        writeControl    writeTime;
        fields          ( h { mean on ; prime2Mean on ; base time ; } );
    }
}


