FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue theta 0.4
);

regions
(
    boxToCell
    {
        box (20 -15 -45) (100 -10 -5);
        fieldValues
        (
            volScalarFieldValue theta 0.4
        );
    }
);


